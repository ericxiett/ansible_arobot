allow booting;
allow bootp;
ddns-update-style none;
ping-check true;
ping-timeout 3;
authoritative;
next-server {{ ips.server }};
filename "pxelinux.0";
option domain-name-servers {{ ips.server }};
option domain-name "inspur.local";
default-lease-time 600;
max-lease-time 7200;

subnet 10.2.16.0 netmask 255.255.240.0 {
range 10.2.17.1 10.2.21.250;
option routers 10.10.31.254;
}
